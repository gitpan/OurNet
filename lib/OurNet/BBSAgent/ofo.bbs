OFO BBS
ofo.twbbs.org:23

=login
wait 名稱
send \n$[username]\n$[password]\nn\n
send \nqq\nqqq
goto main

=main
send eeeeeeee
wait 主功能表
till 通訊器

=balloon
wait \e[24;1H\e[K\e[1;37;46m
till \e[4;33;40m\x20
till \x20\e[0m
exit

=balloon_reply
send \x12
wait 反擊
send $[message]\n
wait [Y]
send \n
wait \e[m
exit

=balloon_send
call talk_users
send /
wait 姓名：
send $[receiver]\n
wait $[receiver]
send w
wait Call-In：
send $[message]\n
wait [Y]
send \n
wait \e[m
exit

=configure
call main
send u\n
wait 生命設定
till 通訊器

=mail
call main
send m\n
wait 曲線通訊
till 通訊器

=talk
call main
send t\n
wait 星光咖啡廳
till 通訊器

=talk_users
call talk
send u\n
wait 休閒聊天
till (TAB/f)

=talk_users_refresh
send \x15
wait 休閒聊天
till (TAB/f)
exit

=mail_send
call mail
send s\n
wait 代號：
send $[receiver]\n
wait 主題：
send $[subject]\n\n
wait 編輯文章
send $[content]\x18
wait 檔案處理
send s\n
wait 底稿
doif $[savelocal]
    send \n
else
    send n\n
endo
send \n

=group
call main
send b\n
wait 看板列表
till 選擇看版
send 1\n

=group_list_first
call group
send 1\npn
exit

=group_list_next
send n
wait ★
till 選擇看版
exit

=board
call group
send /
wait 名稱：
send $[board]\n\n
wait 板主：
till 文章選讀

=board_list_author
call board $[board]
send a$[author]\n
wait ★
till 文章選讀
exit

=board_article_edit
send E$[content]\x18
wait 檔案處理
send s\n
exit

=board_list_last
call board $[board]
send $\nq\n
wait ★
till /
exit

=board_article_select
call board $[board]
send /$[article]\n\nq\n
wait ★
till 文章選讀
exit

=board_article_desc
send q\n
wait ★
till 文章選讀
exit

=board_article_fetch_first
call board $[board]
send /$[article]\n\n
#send \n
till \x20\x20瀏覽\x20P.
wait (
till %)
exit

=board_article_fetch_next
send \x20
wait \e[30m結束
wait \e[0m
till \x20\x20瀏覽\x20P.
wait (
till %)
exit

=logoff
send eeeeeeeeeeee\nn\ny\n\n\n

=chat_enter
call talk
send c\n\n
wait 談天室

=chat_room
send /j $[room]\n
wait [$[room]]
exit

=chat_say
send $[message]\n
wait $[message]
exit

=chat_listen
wait ;1H\x00\x00\x00\x00\x00
till :\x20
  or *\x20
till \e[
exit

=chat_leave
send /bye\n
wait [C]
send m\n
goto main

=chat_msg
send /m $[caller] $[message]\n
wait $[message]
exit

=set_nickname
call talk_users
send N\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f\x7f$[nickname]\n
exit

=board_article_fetch_last
exit
